<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
    <metadata>
        <version>1.5.0</version>
        <id>datatables.net-select-se</id>
        <description>This is Select for DataTables with styling for [SemanticUI](https://semantic-ui.com/)</description>
        <repository type="git" url="https://github.com/DataTables/Dist-DataTables-Select-SemanticUI.git"/>
        <authors>SpryMedia Ltd</authors>
        <projectUrl>http://datatables.net</projectUrl>
        <license type="expression">MIT</license>
        <tags>select  selection  Semantic  SemanticUI  Semantic UI  Datatables jQuery table</tags>
        <dependencies>
            <dependency id="datatables.net-select" version="1.4.0"/>
            <dependency id="datatables.net-se" version="1.12.1"/>
        </dependencies>
        <contentFiles>
            <files include="**/*" buildAction="Content"/>
        </contentFiles>
    </metadata>
    <files>
        <file src="js/select.semanticui.js" target="content\Scripts"/>
        <file src="js/select.semanticui.js" target="contentFiles\any\any\wwwroot\js"/>
        <file src="js/select.semanticui.min.js" target="content\Scripts"/>
        <file src="js/select.semanticui.min.js" target="contentFiles\any\any\wwwroot\js"/>
        <file src="css/select.semanticui.css" target="content\Content"/>
        <file src="css/select.semanticui.css" target="contentFiles\any\any\wwwroot\css"/>
        <file src="css/select.semanticui.min.css" target="content\Content"/>
        <file src="css/select.semanticui.min.css" target="contentFiles\any\any\wwwroot\css"/>
    </files>
</package>

